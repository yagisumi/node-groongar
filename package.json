{
  "name": "@yagisumi/groongar",
  "version": "0.1.0",
  "description": "Groonga client helper.",
  "keywords": [
    "groonga"
  ],
  "private": true,
  "main": "lib/groongar",
  "module": "lib/groongar.mjs",
  "types": "lib/groongar.d.ts",
  "unpkg": "umd/groongar.js",
  "scripts": {
    "test": "run-s prettier:fix eslint test:build test:env:nroonga",
    "test:build": "tsc -p test/src",
    "test:env:nroonga": "jest --testEnvironment=./test/lib/env_nroonga.js",
    "test:env:stdio": "jest --testEnvironment=./test/lib/env_stdio.js --collectCoverage=false",
    "test:env:http": "jest --testEnvironment=./test/lib/env_http.js --collectCoverage=false",
    "build": "run-s clean build:*",
    "clean": "rimraf lib coverage umd *.tgz .rpt2_cache tools/lib test/lib",
    "build:cjs": "rollup -c -f cjs -o ./lib/groongar.js",
    "build:mjs": "rollup -c -f es -o ./lib/groongar.mjs",
    "build:umd": "rollup -c rollup.config.umd.js",
    "build:d_ts": "tsc -p src -d --emitDeclarationOnly --outDir ./lib",
    "prettier:fix": "prettier --write \"**/*.{ts,js,json}\"",
    "eslint": "eslint \"**/*.{js,ts}\"",
    "prepack": "run-s build test",
    "tools:build": "tsc -p tools/src",
    "tools:test": "jest --config tools/jest.config.js",
    "tools:usage": "node tools/lib/tools.js",
    "tools:init": "node tools/lib/tools.js init",
    "tools:sync": "node tools/lib/tools.js sync",
    "tools:groongar": "node tools/lib/tools.js groongar"
  },
  "author": "yagisumi",
  "repository": "github:yagisumi/node-groongar",
  "homepage": "http://github.com/yagisumi/node-groongar#readme",
  "license": "MIT",
  "devDependencies": {
    "@jest/types": "^26.0.1",
    "@types/jest": "^25.2.2",
    "@types/node": "^14.0.1",
    "@types/rimraf": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@yagisumi/groonga-http-client": "file:yagisumi-groonga-http-client-0.1.0.tgz",
    "@yagisumi/groonga-stdio-client": "file:yagisumi-groonga-stdio-client-0.1.0.tgz",
    "coveralls": "^3.1.0",
    "eslint": "^7.0.0",
    "get-port": "^5.1.1",
    "heredocument": "^2.0.2",
    "jest": "^26.0.1",
    "jest-environment-node": "^26.0.1",
    "moment": "^2.25.3",
    "npm-run-all": "^4.1.5",
    "nroonga": "^0.4.3",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.10.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "sortobject": "^4.0.0",
    "ts-jest": "^25.5.1",
    "typescript": "^3.9.2"
  }
}
