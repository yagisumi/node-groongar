{
  "name": "@yagisumi/groongar",
  "version": "0.1.0",
  "description": "",
  "keywords": [],
  "engines": {
    "node": ">= 6.0.0"
  },
  "private": true,
  "main": "lib/groongar",
  "module": "lib/groongar.mjs",
  "types": "lib/groongar.d.ts",
  "unpkg": "umd/groongar.js",
  "scripts": {
    "test": "run-s prettier:fix eslint jest",
    "jest": "jest",
    "doc": "typedoc --json ./docs/typedoc.json --tsconfig ./tsconfig.json ./src",
    "build": "run-s clean build:*",
    "clean": "rimraf lib coverage umd *.tgz .rpt2_cache",
    "build:cjs": "rollup -c -f cjs -o ./lib/groongar.js",
    "build:mjs": "rollup -c -f es -o ./lib/groongar.mjs",
    "build:umd": "rollup -c rollup.config.umd.js",
    "build:d_ts": "tsc -p src -d --emitDeclarationOnly --outDir ./lib",
    "prettier:fix": "prettier --write \"**/*.{ts,js,json}\"",
    "eslint": "eslint \"**/*.{js,ts}\"",
    "prepack": "run-s build test",
    "tools:build": "rollup -c tools/rollup.config.tools.js",
    "tools:test": "jest --config tools/jest.config.js",
    "tools:usage": "node tools/lib/tools.js",
    "tools:init": "node tools/lib/tools.js init",
    "tools:groongar": "node tools/lib/tools.js groongar"
  },
  "author": "yagisumi",
  "repository": "github:yagisumi/node-groongar",
  "homepage": "http://github.com/yagisumi/node-groongar#readme",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.5",
    "@types/rimraf": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.0.0",
    "heredocument": "^2.0.2",
    "jest": "^26.0.1",
    "moment": "^2.25.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.9.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "sortobject": "^3.0.0",
    "ts-jest": "^25.5.1",
    "typedoc": "^0.17.6",
    "typescript": "^3.8.3"
  }
}
