{
  "name": "@yagisumi/groongar",
  "version": "0.1.0",
  "description": "Groonga client helper.",
  "keywords": [
    "groonga"
  ],
  "private": true,
  "main": "lib/groongar",
  "module": "lib/groongar.mjs",
  "types": "lib/groongar.d.ts",
  "unpkg": "umd/groongar.js",
  "scripts": {
    "test": "run-s prettier:fix eslint test:build test:env:nroonga",
    "test:build": "tsc -p test/src",
    "test:env:nroonga": "jest --testEnvironment=./test/lib/env_nroonga.js",
    "test:env:stdio": "jest --testEnvironment=./test/lib/env_stdio.js --collectCoverage=false",
    "test:env:http": "jest --testEnvironment=./test/lib/env_http.js --collectCoverage=false",
    "grntest:env:nroonga": "cross-env NODE_OPTIONS=--max_old_space_size=4096 jest --logHeapUsage --config=jest.grntest.config.js --testEnvironment=./test/lib/env_nroonga.js",
    "grntest:mocha:nroonga": "cross-env TS_NODE_FILES=true mocha --reporter json --timeout 30000 -r ts-node/register -r tsconfig-paths/register -r ./test/lib/mocha_env_nroonga \"test/grntest/suite/**/*.i-test.ts\"",
    "build": "run-s clean build:*",
    "clean": "rimraf lib coverage umd *.tgz .rpt2_cache tools/lib test/lib",
    "build:cjs": "rollup -c -f cjs -o ./lib/groongar.js",
    "build:mjs": "rollup -c -f es -o ./lib/groongar.mjs",
    "build:umd": "rollup -c rollup.config.umd.js",
    "build:d_ts": "tsc -p src -d --emitDeclarationOnly --outDir ./lib",
    "prettier:fix": "prettier --write \"**/*.{ts,js,json}\"",
    "eslint": "eslint \"**/*.{js,ts}\"",
    "prepack": "run-s build test",
    "tools:build": "tsc -p tools/src",
    "tools:test": "jest --config tools/jest.config.js",
    "tools:usage": "node tools/lib/tools.js",
    "tools:init": "node tools/lib/tools.js init",
    "tools:example": "node tools/lib/tools.js example",
    "tools:convert": "node tools/lib/tools.js convert",
    "tools:groongar": "node tools/lib/tools.js groongar"
  },
  "author": "yagisumi",
  "repository": "github:yagisumi/node-groongar",
  "homepage": "http://github.com/yagisumi/node-groongar#readme",
  "license": "MIT",
  "devDependencies": {
    "@jest/types": "^26.3.0",
    "@types/chai": "^4.2.12",
    "@types/jest": "^26.0.13",
    "@types/levelup": "^4.3.0",
    "@types/mkdirp": "^1.0.1",
    "@types/node": "^14.6.4",
    "@types/rimraf": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "@yagisumi/groonga-command": "^0.1.0",
    "@yagisumi/groonga-http-client": "file:yagisumi-groonga-http-client-0.1.0.tgz",
    "@yagisumi/groonga-stdio-client": "file:yagisumi-groonga-stdio-client-0.1.0.tgz",
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.8.1",
    "get-port": "^5.1.1",
    "heredocument": "^2.0.2",
    "jest": "^26.4.2",
    "jest-environment-node": "^26.3.0",
    "level": "^6.0.1",
    "mkdirp": "^1.0.4",
    "mocha": "^8.1.3",
    "moment": "^2.27.0",
    "npm-run-all": "^4.1.5",
    "nroonga": "^0.4.3",
    "prettier": "^2.1.1",
    "readdirp": "^3.4.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.26.10",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.27.2",
    "sortobject": "^4.13.0",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.2"
  }
}
